{
  "name": "entity-reporting-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/setup-prisma.js && npm run build:client",
    "build:client": "vite build",
    "build:server": "node scripts/setup-prisma.js",
    "preview": "vite preview",
    "seed": "tsx server/prisma/seed.ts",
    "prisma:generate": "npx prisma generate --schema=server/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate deploy --schema=server/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=server/prisma/schema.prisma"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.0",
    "xlsx": "^0.18.5",
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "serverless-http": "^3.2.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6"
  }
}